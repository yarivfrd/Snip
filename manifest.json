{
  "manifest_version": 2,

  "name": "Snip",
  "description": "Visual clipboard.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "version": "1.0",
  "author": "Yairv Fruend",

  "browser_action": {
    "default_icon": "icons/32.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}